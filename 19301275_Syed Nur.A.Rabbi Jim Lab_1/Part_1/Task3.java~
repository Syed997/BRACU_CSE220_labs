import java.util.*;
public class Task3{
  public static void main(String [] args){
    Scanner input= new scsnner(System.in);
    int [] a= new int[5];
    a[i]=input.nextInt();
    for(int i=1; i<a.length; i++){
      a[i]=input.nextInt();
      if(a[i]<a[i-1]){
        int temp=a[i];
        a[i]=a[i-1];
        a[i=1]=temp;
      }
    }
    for(int i=0; i<a.length;i++)
      System.out.print(a[i]);
  }
  for(int i=0; i<a.length;i++){
    if(a[i]%2==0){
      System.out.print(a[i]);
    }
  }
}